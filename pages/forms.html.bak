                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Cadastro Básico
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
								<h1>Cadastro do Paciente</h1>
                                    <form role="form">
                                        <div class="form-group">
										    <label>Nome do Paciente</label>
                                            <input class="form-control">
                                       </div>
                                        <div class="form-group input-group">
		                                    <span class="input-group-addon">@</span> 
                                            <input type="text" class="form-control" placeholder="Digite seu e-mail">
                                        </div>
										<div class="form-group">
                                            <label>Informe número do celular</label>
                                            <input class="form-control" placeholder="(DDD)">
                                        </div>
										 <button type="submit" class="btn btn-primary">Alterar Senha</button>
                                        <button type="submit" class="btn btn-default">Apagar</button>
								
								<h1>Cadastro do Cuidador</h1>		
										<div class="form-group">
										    <label>Nome do Cuidador</label>
                                            <input class="form-control">
                                       </div>
                                        <div class="form-group input-group">
		                                    <span class="input-group-addon">@</span> 
                                            <input type="text" class="form-control" placeholder="Digite seu e-mail">
                                        </div>
										<div class="form-group">
                                            <label>Informe número do celular</label>
                                            <input class="form-control" placeholder="(DDD)">
                                        </div>
										<div class="form-group">
                                            <label>Observação / Detalhes</label>
                                            <textarea class="form-control" rows="3"></textarea>
                                        </div>
                                       											
									    <button type="submit" class="btn btn-primary">Alterar Senha</button>
                                        <button type="submit" class="btn btn-default">Apagar</button>
                                       
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                                <div class="col-lg-6">
                                    <h1>Cadastro do Medicamento</h1>
                                    <form role="form">
                                        
										
	<!-- Cadastro de Medicamentos   -->	
										
                                    <form role="form">
                                        <div class="form-group">
										    <label>Nome do Medicamento</label>
                                            <input class="form-control">
                                       </div>
										<div class="form-group">
                                            <label>Anexar Receita</label>
                                            <input type="file">
                                        </div>
                                        <div class="form-group">
                                            <label>Observação / Detalhes</label>
                                            <textarea class="form-control" rows="3"></textarea>
                                        </div>
										
										
										<!-- Cadastro de horario   -->		
										<h1>Cadastrar Alertas</h1>				
										<label>Data de Início</label> 
										  <div class="col-md-6 input-group">
												<input type="text" id="testdate" name="testdate" class="form-control" value="dd/mm/aaaa">
												<label class="input-group-addon btn" for="testdate">
													<span class="fa fa-calendar"></span>
												</label>                    
										  </div>
										
										<label>Horário do Medicamento</label>
											<div class="col-md-6 input-group">
												<div class="input-group bootstrap-timepicker timepicker">
														<input id="timepicker4" type="text" value="10:35 AM" class="form-control input-small">
														<span class="input-group-addon"><i class="glyphicon glyphicon-time"></i></span>
												</div>
											</div>
											<script type="text/javascript">
												$('#timepicker4').timepicker({
													minuteStep: 1,
													secondStep: 5,
													showInputs: false,
													template: 'modal',
													modalBackdrop: true,
													showSeconds: true,
													showMeridian: false
												});
											</script>
<br>
											<label>Padrão de Recorrência</label>
											<div class="container">
											  <form>
												<label class="radio-inline">
												  <input type="radio" name="optradio">Diário
												</label>
												<label class="radio-inline">
												  <input type="radio" name="optradio">Semanal
												</label>
												<label class="radio-inline">
												  <input type="radio" name="optradio">Mês
												</label>
												  <label class="radio-inline">
												  <input type="radio" name="optradio">Anual
												</label>
											  </form>
											</div>
										
<br>										
										<label>Dias da Semana</label>
                                        <div class="form-group">
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Domingo
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Segunda-Feira
                                                </label>
                                            </div>
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Terça-Feira
                                                </label>
                                            </div>
											<div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Quarta-Feira
                                                </label>
                                            </div>
											<div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Quinta-Feira
                                                </label>
                                            </div>
											<div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Sexta-Feira
                                                </label>
                                            </div>
											<div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="">Sábado-Feira
                                                </label>
                                            </div>
                                        </div>
													
										<button type="submit" class="btn btn-primary">Salvar</button>
                                        </fieldset>
                                    </form>
                                    
